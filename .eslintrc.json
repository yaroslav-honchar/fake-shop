{
  "extends": "next/core-web-vitals",
  "ignorePatterns": [
    "**/*.config.js"
  ],
  "rules": {
    "react/display-name": "off",
    "no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ],
    "semi": [
      "error",
      "never"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "react/no-unescaped-entities": "off",
    "@next/next/no-img-element": "off",
    "no-duplicate-imports": "error"
  }
}