{
  "name": "fake-shop",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "fix:eslint": "eslint src/**/*.{ts,tsx} --fix --config .eslintrc.json",
    "fix:stylelint": "stylelint src/**/*.css --fix",
    "lint": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "npm run fix:eslint",
      "git add ."
    ],
    "*.css": [
      "npm run fix:stylelint",
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.9.0",
    "axios": "^1.5.0",
    "classnames": "^2.3.2",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.1.2"
  },
  "devDependencies": {
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "editorconfig-checker": "^5.1.1",
    "eslint": "^8.50.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^4.3.8",
    "i": "^0.3.7",
    "lint-staged": "^14.0.1",
    "npm": "^10.2.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "5.1.6"
  }
}
